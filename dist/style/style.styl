.maximize .get-code-window .json-editor,
.maximize .get-code-window .bookmark-desc-editor
  margin: 0 -11px !important;
.get-code-window
  // position: relative
  background: #FFF
  padding: 1em 3em
  padding-bottom: 0
  width: auto
  .J-editors-tabs
    .tab-titles
      display: inline-block;
      .tab-title
        &.active
          background: #272822
          color: white
        cursor: pointer
        display: inline-block;
        padding: 6px 6px;
        font-size: 14px;
        color: black
  h5
    padding: 0 0 15px
    font-size: 20px
  .layui-form
    max-width: 500px
    input
      display: inline-block
  .CodeMirror-scroll
    min-height: 15em
  .actions
    position: absolute
    right: 2px;
    top: 0px;
    .cm-btn
      height: 36px;
      line-height: 36px
      cursor: pointer;

  .CodeMirror-hscrollbar
    outline: none

.json-editor
  .cm-em
    font-style: normal    

.websites
  width: 100%
  float: left
  
  .website
    position: relative
    float: left
    height: 50px
    width: 25%
    padding: 10px 15px
    border-bottom: 1px solid rgba(120,130,140,.065)
    display: flex
    align-items: center
    padding: 10px 15px
    .Mobile &
      padding: 10px 6px
    .hasDropdown
      position: absolute;
      color: #a6abab;
      font-size: 0.8em;
      left: 0.2em;
      bottom: 1.4em;
      .Mobile &
        &:before
          content: "\e62f"
        bottom: 0.5em;
        left: 1em;
        
    .J-holder
      display: block;
      width: 50%;
      position: absolute;
      right: 0;
      top: 0;
      background: #ececec;
      height: 100%;
      cursor: pointer;
    &.grayBg
      background: #f3f6f97a
    > .link
      display: inline-block
      overflow: hidden
      white-space: nowrap
      text-overflow: ellipsis
      height: 100%
      line-height: 28px
      // min-width: 60%
      max-width: 100%

      .Mobile &
        font-size: 12px
      &:hover + .sub
        // display: block
    .hasSubList
      position: relative
    .sub
      min-width: 200px
      max-width: 350px
      display: none
      position: absolute
      left: 10px
      top: 35px
      background: white
      z-index: 2
      padding: 10px 0
      box-shadow: 0 6px 12px rgba(0,0,0,.175);
      border-top: 2px solid #252839;
      .folder-arrow
        position: absolute
        right: 0.5em
      > .sub
        display: none
        > .subList
          display: none;
      .closeDropdown
        display: none;
        position: absolute;
        right: 0em;
        background: white;
        width: 30px;
        text-align: center;
        height: 30px;
        font-size: 18px;
        cursor: pointer
        z-index: 9
        .mobile &
          display: inline-block;
        .Mobile &
          font-size: 12px
      > .linkGroup:nth-child(2n+1)
        > .link
          background: #ececec7a;
      .link
        padding: 5px 15px
        display: block
        transition: all 0.2s ease
        font-size: 14px
        word-break: break-all;
        &:hover
          color: white
          background-color: #252839 !important
