/* Begin cm
================================================== */
.cm-formLayout
  .rows
    padding: 10px 0
    .row
      position: relative
      padding-left: 150px
      .title, .label
        position: absolute
        left: 0
        top: 0
        width: 140px
        text-align: right
        label
            height: 38px
            line-height: 38px
            display: inline-block
      .input
        input[type=text]
          display: block
          width: 100%
          padding-left: 10px
          height: 38px
          line-height: 1.3
          line-height: 38px
          border-width: 1px
          border-color: #e6e6e6
          border-style: solid
          background-color: #fff
          border-radius: 2px
          &:focus
            border-color: #C9C9C9 !important

.cm-dialog-wrapper
  position: fixed
  width: 100%
  height: 100%
  top: 0
  left: 0
  text-align: center
  display: flex
  align-items: center
  background: rgba(0,0,0,.5)
  z-index: 1000
  .cm-dialog
    margin: auto
    background: white
    padding: 15px
    text-align: left
    .title
      padding-bottom: 15px
    .content
      padding-bottom: 15px
    // .actions

.cm-popupDialog
  .actions
    position: absolute;
    right: 2px;
    top: 0px;
    .cm-btn
      height: 36px;
      line-height: 36px;
      cursor: pointer;

/* End cm
================================================== */





/* Begin dash
================================================== */

.fastSeach
  position: relative
  flex-grow: 1
  margin-right: 1em
  .searchWebsites
    display block
  .clearText
    position absolute
    right 0.5em
    top 0.7em
    cursor pointer
    transform: scale(1.32)
    // display none
  .results
    position: absolute
    left: 0
    top: 40px
    background: white
    z-index: 10
    width: 100%
    padding: 5px 0
    border: 1px solid #eee
    // display: none
    .flex
      display flex
    .searchItems
      width 70%
      .item
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
        b
          color: red
        &:nth-child(2n)
          background #eee
    .others
      width 30%
      .item
        &:nth-child(2n+1)
          background #eee
    .item
      padding: 5px
      display: block
      font-size: 12px
      word-break: break-all;
      padding-top 5px;
      cursor pointer
      &.history
        color: #cacaca
      &.active, &:hover
        color: white
        background: #252839 !important
/* End dash
================================================== */


.getUserSecret
  padding 10px
  > *
    margin-bottom 10px
  label
    display block
    


.needLogin
  padding 15px
  min-height 15em
  display flex
  align-items center
  justify-content: center;
  p
    display block    

.textarea {
  font-family: Consolas, monospace;
  white-space: pre;
  word-wrap: normal;
  padding: 0.1em 0.3em;
  border: solid thin;
  overflow: auto;
  width: 100%;
}

.mainMenu
  a
    margin-right 1em




